{
  // Visit https://aka.ms/tsconfig to read more about this file
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",                 /* <-- Ubah ini */
    "moduleResolution": "Node",           /* <-- Tambahkan/ubah ini */
    "esModuleInterop": true,              /* <-- Pastikan ini 'true' */
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
